package pertemuan06_List;

public class StrukturListTest {
	public static void main(String[] args) {
		StrukturList list = new StrukturList();
		
		// Latihan 2
		System.out.println("Latihan 3");
		list.addHead(2);
		list.addHead(9);
		list.addHead(7);
		
		list.displayElement();
		
		list.removeHead();
		
		list.displayElement();
		
		list.removeHead();
		
		list.displayElement();
		
		list.removeHead();
		
		list.displayElement();

		// Latihan 4
		System.out.println("Latihan 4");
		list = new StrukturList();
		list.addHead(2);
		list.addTail(6);
		list.addTail(3);
		list.addTail(5);
		list.addTail(1);
		
		list.removeTail();
		
		list.displayElement();
		
		list.removeHead();
		
		list.displayElement();
		
		list.removeTail();
		
		list.displayElement();

        
		//Latihan 5
		System.out.println("Latihan 5-1");
		list = new StrukturList();
		list.addHead(4);
		list.addTail(3);
		list.addTail(7);
		list.addTail(8);
		list.addTail(1);
		list.addTail(9);
		
		list.displayElement();
		
		list.removeMiddle(8);
		
		list.displayElement();
		
		list.removeMiddle(3);
		
		list.displayElement();
		
		list.removeTail();
		
		list.displayElement();
		
		System.out.println("\nLatihan 5-2");
		list = new StrukturList();
		list.addHead(2);
		list.addTail(3);
		list.addTail(4);
		list.addTail(8);
		list.addTail(1);
		list.addTail(9);
		
		list.displayElement();
		
		list.removeMiddle(8);
		
		list.displayElement();
		
		list.removeMiddle(3);
		
		list.displayElement();
		
		list.removeTail();
		
		list.displayElement();
		
    }
}