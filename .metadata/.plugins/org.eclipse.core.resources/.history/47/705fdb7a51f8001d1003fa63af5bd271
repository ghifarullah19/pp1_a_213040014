package pertemuan10_Stack;

public class StackMain {
    public static void main(String[] args) {
        StrukturStack stack = new StrukturStack(3);

        // Latihan 4
        System.out.println("### Sebelum Enstack 3x ###");
        System.out.println("size: " + stack.size());
        System.out.println("isEmpty: " + stack.isEmpty());
        stack.displayElement();

        // Latihan 4
        stack.enstack(3);
        stack.enstack(2);
        stack.enstack(1);

        // Latihan 4
        System.out.println("\n### Sesudah Enstack 3x ###");
        System.out.println("size: " + stack.size());
        System.out.println("isEmpty: " + stack.isEmpty());
        stack.displayElement();
        System.out.println("Front: " + stack.front());

//        stack = new StrukturStack();
